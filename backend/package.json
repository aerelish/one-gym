{
	"name": "one-gym-backend",
	"version": "1.0.0",
	"description": "Backend for One Gym application",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"dev": "tsx --watch --env-file .env src/server.ts",
		"start": "node --env-file .env dist/server.js",
		"type-check": "tsc --noEmit",
		"prisma:migrate": "prisma migrate dev",
		"prisma:generate": "prisma generate",
		"test": "jest",
		"test:clear": "jest --clearCache",
		"test:watch": "jest --watch"
	},
	"keywords": [],
	"author": "aerelish",
	"license": "Portfolio License",
	"type": "module",
	"imports": {
		"#*": "./src/*"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^7.4.0",
		"@prisma/client": "^7.4.0",
		"bcrypt": "^6.0.0",
		"cors": "^2.8.6",
		"express": "^5.2.1",
		"jsonwebtoken": "^9.0.3"
	},
	"devDependencies": {
		"@prisma/adapter-better-sqlite3": "^7.4.0",
		"@tsconfig/node22": "^22.0.5",
		"@types/bcrypt": "^6.0.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.6",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^25.2.3",
		"@types/pg": "^8.16.0",
		"@types/supertest": "^6.0.3",
		"jest": "^30.2.0",
		"prisma": "^7.4.0",
		"supertest": "^7.2.2",
		"ts-jest": "^29.4.6",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0"
	}
}
